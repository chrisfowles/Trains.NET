<Project>

  <PropertyGroup>
    <RestoreSources>
      https://api.nuget.org/v3/index.json;
    </RestoreSources>
  </PropertyGroup>
  
  <ItemGroup>
    
    <PackageReference Update="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.8" />

    <!-- DI -->
    <PackageReference Update="Microsoft.Extensions.DependencyInjection" Version="3.1.0" />

    <!-- SkiaSharp -->
    <PackageReference Update="SkiaSharp"                    Version="1.68.1" />
    <PackageReference Update="SkiaSharp.Views.WindowsForms" Version="1.68.1" />
    <PackageReference Update="SkiaSharp.Views.WPF"          Version="1.68.1" />
    
    <!-- Comet 
              <PackageReference Update="Clancey.Comet"                Version="0.2.328-beta" />
              <PackageReference Update="Clancey.Comet.Skia"           Version="0.2.328-beta" />
    -->

    <!-- Nerdbank GitVresioning - Should be in all projects -->
    <PackageReference Include="Nerdbank.GitVersioning" Version="3.1.91">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
    
  </ItemGroup>
  
</Project>